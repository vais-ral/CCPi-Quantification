$(INTERMEDIATEDIR)/CCPiParticleTracker.d: CCPiParticleTracker.cpp $(UICOUTPUT) $(filter %.h, $(COPY))
	@echo "    [$(MAKE_CFG)] Generating dependencies for $(DIRNAME)/$<"
	@mkdir -p $(@D)
	$(QUIET)$(MAKEDEP)
	@cp $@ $@.orig
	@sed -e 's/\.o:/.d:/' <$@.orig >>$@
	@sed -e 's/#.*//' -e 's/^[^:]*: *//' -e 's/ *\\$$//' -e '/^$$/ d'  -e 's/^ *//' -e 's/$$/ :/' <$@.orig >>$@
	@rm $@.orig

DEPS += $(INTERMEDIATEDIR)/CCPiParticleTracker.d
endif

