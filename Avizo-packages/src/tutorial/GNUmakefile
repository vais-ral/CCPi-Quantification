# The C and CPP targets.
$(INTERMEDIATEDIR)/CCPiAccessibleVolume.o: CCPiAccessibleVolume.cpp $(UICOUTPUT) $(filter %.h, $(COPY))
	@echo "    [$(MAKE_CFG)] Compiling $(DIRNAME)/$<"
	@mkdir -p $(@D)
	$(QUIET)$(CXX) $(CXXFLAGS) -c $< -o $@

OBJS += $(INTERMEDIATEDIR)/CCPiAccessibleVolume.o

ifdef MAKEDEP
$(INTERMEDIATEDIR)/CCPiAccessibleVolume.d: CCPiAccessibleVolume.cpp $(UICOUTPUT) $(filter %.h, $(COPY))
	@echo "    [$(MAKE_CFG)] Generating dependencies for $(DIRNAME)/$<"
	@mkdir -p $(@D)
	$(QUIET)$(MAKEDEP)
	@cp $@ $@.orig
	@sed -e 's/\.o:/.d:/' <$@.orig >>$@
	@sed -e 's/#.*//' -e 's/^[^:]*: *//' -e 's/ *\\$$//' -e '/^$$/ d'  -e 's/^ *//' -e 's/$$/ :/' <$@.orig >>$@
	@rm $@.orig

DEPS += $(INTERMEDIATEDIR)/CCPiAccessibleVolume.d
endif

